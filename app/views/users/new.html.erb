<div class="main users-new">
  <div class="container">
    <div class="users-heading">・新規ユーザー登録</div>
    <div class="form users-new-form">
      <div class="form-body">

        <%= form_tag("/users/create") do %>
        <div>
          <p>社員No.を入力してください</p>
          <input name="enum2" value="<%= @enum2 %>">
         <input type="submit" value="新規登録">
        </div>
       <% end %>
      </div>
    </div>
  </div>
</div>
         <% if @error_message %>
           <div class="login-form-error">
             <%= @error_message %>
           </div>
         <% end %>



        <%= form_tag("/users/create", multipart: true) do %>
        <div>
          <p>社員No.を入力してください</p>
          <input name="enum2" value="<%= @enum2 %>">
          <%= file_field_tag :file %>
         <input type="submit" value="新規登録">
        </div>
       <% end %>